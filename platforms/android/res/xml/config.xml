<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.clazzmanager" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <splash density="land-hdpi" src="www/img/H_HDPI.png" />
    <splash density="land-ldpi" src="www/img/H_LDPI.png" />
    <splash density="land-mdpi" src="www/img/H_MDPI.png" />
    <splash density="land-xhdpi" src="www/img/H_XHDPI.png" />
    <splash density="port-hdpi" src="www/img/V_HDPI.png" />
    <splash density="port-ldpi" src="www/img/V_LDPI.png" />
    <splash density="port-mdpi" src="www/img/V_MDPI.png" />
    <splash density="port-xhdpi" src="www/img/V_XHDPI.png" />
    <icon density="mdpi" src="www/img/icone128.png" />
    <icon density="hdpi" src="www/img/icone128.png" />
    <icon density="xhdpi" src="www/img/icone128.png" />
    <name>ClazzManager</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
    <icon density="ldpi" src="www/img/icone128.png" />
    <content src="index.html" />
    <access origin="*" />
</widget>
